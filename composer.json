{
	"name": "getkirby/demo-manager",
	"description": "Demo manager at https://trykirby.com",
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Kirby Team",
			"email": "support@getkirby.com",
			"homepage": "https://getkirby.com"
		}
	],
	"require": {
		"php": ">=8.1.0",
		"getkirby/cms": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"Kirby\\": "src/"
		}
	},
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	},
	"extra": {
		"kirby-cms-path": false
	}
}
